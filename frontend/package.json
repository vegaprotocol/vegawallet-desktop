{
  "name": "Vega Wallet",
  "author": "Gobalsky Labs Ltd.",
  "version": "0.11.2",
  "private": true,
  "engines": {
    "node": "^18.12.0"
  },
  "scripts": {
    "build:test": "VITE_COVERAGE='true' npx vite build",
    "build": "npx vite build",
    "dev:test": "cd .. && VITE_COVERAGE='true' VITE_FEATURE_NETWORK_WARNING='false' VITE_FEATURE_TELEMETRY_CHECK='false' wails dev -noreload",
    "dev:test:ci": "cd .. && VITE_COVERAGE='true' VITE_FEATURE_NETWORK_WARNING='false' VITE_FEATURE_TELEMETRY_CHECK='false' wails dev -s -skipbindings -nosyncgomod -noreload",
    "dev": "VITE_ENV='development' && npx vite --port=3000",
    "e2e:clean": "rm -r automation/test-wallets; mkdir automation/test-wallets",
    "e2e:open": "yarn e2e:run --headed",
    "e2e:run": "yarn playwright test -c automation/playwright.config.ts",
    "format:check": "npx prettier --check .",
    "format:fix": "npx prettier --write .",
    "lint:check": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "dependencies": {
    "@sentry/react": "7.19.0",
    "@sentry/tracing": "7.19.0",
    "@vegaprotocol/wallet-admin": "0.2.0",
    "@vegaprotocol/wallet-ui": "0.2.2",
    "core-js": "3.26.1",
    "immer": "9.0.15",
    "loglevel": "1.8.1",
    "nanoid": "4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@ethersproject/abstract-signer": "5.7.0",
    "@ethersproject/experimental": "5.7.0",
    "@ethersproject/providers": "5.7.2",
    "@ethersproject/wallet": "5.7.0",
    "@playwright/test": "^1.32.1",
    "@types/lodash": "4.14.189",
    "@types/node": "^18.14.0",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "2.2.0",
    "axios": "^1.3.2",
    "deepmerge": "^4.2.2",
    "esbuild": "^0.15.15",
    "eslint": "^8.33.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-playwright": "^0.12.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-testing-library": "5.9.1",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "lodash": "^4.17.21",
    "playwright": "^1.32.1",
    "prettier": "2.7.1",
    "resize-observer-polyfill": "^1.5.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "vite": "3.2.4",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-istanbul": "3.0.2",
    "webpack": "^5.75.0"
  }
}
