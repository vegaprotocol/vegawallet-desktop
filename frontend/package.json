{
  "name": "vegawallet",
  "author": "Gobalsky Labs Ltd.",
  "version": "0.5.0",
  "private": true,
  "engines": {
    "node": "^18.12.0"
  },
  "scripts": {
    "dev": "VITE_ENV='development' && npx vite --port=3000",
    "dev:test": "cd .. && VITE_COVERAGE='true' VITE_FEATURE_NETWORK_WARNING='false' VITE_FEATURE_TELEMETRY_CHECK='false' wails dev -noreload",
    "build": "npx vite build",
    "build:test": "VITE_COVERAGE='true' npx vite build",
    "test": "jest",
    "format:check": "npx prettier --check .",
    "format:fix": "npx prettier --write .",
    "lint:check": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@sentry/react": "7.19.0",
    "@sentry/tracing": "7.19.0",
    "@vegaprotocol/wallet-client": "0.0.6",
    "@vegaprotocol/wallet-ui": "^0.1.17",
    "core-js": "3.26.1",
    "jest-environment-jsdom": "29.3.1",
    "loglevel": "1.8.1",
    "nanoid": "4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@cypress/instrument-cra": "1.4.0",
    "@types/lodash": "4.14.189",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "2.2.0",
    "esbuild": "^0.15.15",
    "eslint": "8.27.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-jest": "27.1.5",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-testing-library": "5.9.1",
    "jest": "29.3.1",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "vite": "3.2.4",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-istanbul": "3.0.2"
  }
}
